<Window x:Class="MVVM_Example.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"        
        xmlns:ViewModel="clr-namespace:MVVM_Example.ViewModel"
        xmlns:View="clr-namespace:MVVM_Example.View"
        mc:Ignorable="d"
        Title="MainWindow" MinWidth="300" ResizeMode="NoResize" SizeToContent="WidthAndHeight">
    
    <Window.DataContext>
        <ViewModel:Presenter/>
    </Window.DataContext>

    <View:ConverterControl/>
    
</Window>
